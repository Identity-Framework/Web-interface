<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
   <constant name="struts.devMode" value="true" />
   <package name="helloworld" namespace="/" extends="json-default">   
      <action name="query" 
            class="edu.ncat.struts2.SPARQLAction" 
            method="execute">
            <result name="success">/viewresults.jsp</result>
            <result name="error">/queryError.jsp</result>
      </action>
      <action name="description" class="edu.ncat.struts2.ScenarioDescriptionAction" method="execute">
      	<result type="json">
      		<param name="excludeNullProperties">true</param>
        	<param name="noCache">true</param>
      	</result>
      </action>
      <action name="evidence" class="edu.ncat.struts2.DempsterShaferAction" method="execute">
      	<result name="success">/viewEvidence.jsp</result>
      </action>
   </package>
</struts>